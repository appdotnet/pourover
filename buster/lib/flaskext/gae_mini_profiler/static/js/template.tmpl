<script id="profilerCornerTemplate" type="text/x-jquery-tmpl">
    <div class="g-m-p-corner"></div>
</script>

<script id="profilerCornerEntryTemplate" type="text/x-jquery-tmpl">
    <div class="entry">
        ${profiler_results.total_time} <span class="ms">ms</span>
    </div>
